{
  "name": "@cursorly/web-components",
  "version": "0.0.6-alpha.4",
  "description": "Cursorly web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components/web-components.esm.js",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nemwiz/cursorly-web-components"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "npm-run-all --parallel start:stencil start:server",
    "start:stencil": "stencil build --watch --serve",
    "start:server": "http-server dist -p 5555 --cors",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/hands": "^0.4.1675469240",
    "@mediapipe/tasks-vision": "0.1.0-alpha-7",
    "@stencil/core": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@stencil/angular-output-target": "^0.6.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.15.11",
    "babel-loader": "^9.1.2",
    "http-server": "^14.1.1",
    "jest": "^27.5.1",
    "jest-cli": "^29.5.0",
    "lit-html": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.9.1"
  },
  "license": "MIT"
}
